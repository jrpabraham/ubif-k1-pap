@article{asfaw_cash_2014,
	title = {Cash transfer programme, productive activities and labour supply: Evidence from randomized experiment in Kenya},
	volume = {50},
	issn = {0022-0388},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC4316733/},
